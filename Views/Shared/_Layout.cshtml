<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <link href="~/Content/Site.css" rel="stylesheet" type="text/css" />
    <!-- CSS only -->
    <link rel="stylesheet" href="~/Content/themes/base/jquery-ui.css" />
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/css/bootstrap.min.css" integrity="sha384-9aIt2nRpC12Uk9gS9baDl411NQApFmC26EwAOH8WgZl5MYYxFfc+NcPb1dKGj7Sk" crossorigin="anonymous">

    <script src="~/Scripts/modernizr-2.8.3.js"></script>
    <script src="https://code.jquery.com/jquery-3.5.1.min.js" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script>
    <script src="~/Scripts/jquery-ui-1.12.1.js"></script>

   
   </head>
<body >
        <h1 class="badge-pill badge-danger col-md-5">Concursos y Contratos</h1>
    <nav style="background-color:lavender" class="navbar navbar-fixed-top navbar-dark bg-dark navbar-expand-lg ">
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNavDropdown" aria-controls="navbarNavDropdown" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse justify-content-center " id="navbarNavDropdown">
            <ul class="nav  ">
                <li class="nav-item">
                    @Html.ActionLink(" ", "Index", "Usuario", new { area = "" }, new { @class = "text-center navbar-brand nav-link fas fa-home" })
                </li>
                <li class="nav-item">
                    @Html.ActionLink("Usuario", "Index", "Usuario", new { area = "" }, new { @class = "navbar-brand nav-link " })
                </li>
                <li class="nav-item">
                    @Html.ActionLink("Licitaciones", "Index", "Usuario", new { area = "" }, new { @class = "navbar-brand nav-link " })
                </li>
                <li class="nav-item">
                    @Html.ActionLink("Oficios Autorizacion", "Index", "Usuario", new { area = "" }, new { @class = "navbar-brand nav-link " })
                </li>
                <li class="nav-item">
                    @Html.ActionLink("Contratistas", "Index", "Usuario", new { area = "" }, new { @class = "navbar-brand nav-link " })
                </li>
            </ul>
        </div>
    </nav>


    <div class="container-fluid body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </footer>
    </div>

    <script src="https://kit.fontawesome.com/dd4a8d0f3f.js" crossorigin="anonymous"></script>
    <!-- JS, Popper.js, and jQuery -->
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@9"></script>

    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/js/bootstrap.min.js" integrity="sha384-OgVRvuATP1z7JjHLkuOU7Xw704+h835Lr+6QL9UvYjZE3Ipu6Tp75j7Bh/kR0JKI" crossorigin="anonymous"></script>
    <script src="~/scripts/jquery.unobtrusive-ajax.js"></script>


</body>
</html>